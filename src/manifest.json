{
  "name": "gooreplacer",
  "version": "0.2",
  "description": "A replacer for google fonts/api/themes.... to load page faster!",
  "permissions": [
      "webRequest", "webRequestBlocking",
      "storage",
      "downloads",
      "http://*/*",
      "https://*/*"
  ],
  "background": {
    "scripts": [ "db.js" ,"background.js"]
  },
  "icons": {
    "32": "data/gooreplacer.ico",
    "16": "logo/16.png",
    "48": "logo/48.png",
    "128": "logo/128.png"
  },
  "options_page": "data/options.html",
  "author": "http://liujiacai.net",
  "homepage_url": "https://github.com/jiacai2050/gooreplacer4chrome",
  "update_url": "https://github.com/jiacai2050/gooreplacer4chrome/raw/master/updates.xml",
  "manifest_version": 2
}
